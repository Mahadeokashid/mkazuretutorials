{
	"name": "apitosqlserver",
	"properties": {
		"activities": [
			{
				"name": "apitoazuredatamovement",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureFileStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['name']"
								},
								"sink": {
									"name": "name"
								}
							},
							{
								"source": {
									"path": "$['email']"
								},
								"sink": {
									"name": "email"
								}
							},
							{
								"source": {
									"path": "$['gender']"
								},
								"sink": {
									"name": "gender"
								}
							},
							{
								"source": {
									"path": "$['status']"
								},
								"sink": {
									"name": "status"
								}
							},
							{
								"source": {
									"path": "$['created_at']"
								},
								"sink": {
									"name": "created_at"
								}
							},
							{
								"source": {
									"path": "$['updated_at']"
								},
								"sink": {
									"name": "updated_at"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "dsinputrestapi",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "apidata",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}